<style>
@-webkit-keyframes rainbow {
  0% {border-color: hsl(0, 100%, 50%);}
  14% {border-color: hsl(30,100%,50%);}
  28% {border-color: hsl(60,100%,50%);}
  42% {border-color: hsl(120,100%,50%);}
  56% {border-color:  hsl(240,100%,50%);}
  70% {border-color: hsl(280,100%,50%);}
  84% {border-color: hsl(320,100%,50%);}
  100% {border-color: hsl(255, 100%, 50%);}
}
@-webkit-keyframes rainbow-text {
  0% {color: hsl(0, 100%, 50%);}
  14% {color: hsl(30,100%,50%);}
  28% {color: hsl(60,100%,50%);}
  42% {color: hsl(120,100%,50%);}
  56% {color:  hsl(240,100%,50%);}
  70% {color: hsl(280,100%,50%);}
  84% {color: hsl(320,100%,50%);}
  100% {color: hsl(255, 100%, 50%);}
}
.text-animation {
-webkit-animation: rainbow-text 5s infinite alternate; 
}
.border-animation {
-webkit-animation: rainbow 5s infinite alternate; 
}
</style>

<% bot.guilds.cache.get(config.server.id).members.cache.filter(a => a.roles.cache.has(roles.sitecreator)).map(a => {%>
<div class="disbots_column">
<div class="disbots_teamcard border-animation">
<div class="avatar">
<img draggable="false" class="pulse-avatar-<%=bot.users.cache.get(a.id).presence.status%>" src="<%=bot.users.cache.get(a.id).avatarURL({dynamic: true})%>">
<div class="user">
<h1><%=a.user.username%></h1>
<h1><%=a.user.id%></h1>
<h2 class="text-animation" ><%=bot.guilds.cache.get(config.server.id).roles.cache.get(roles.sitecreator).name%></h2>
</div>
</div>
<div class="disbots_teamcard-footer">
<a href="https://disbots.xyz/user/<%=a.id%>" class="btn btn-tag btn-block"><i class="fad fa-eye"></i>&nbsp;View</a>
</div>
</div>
</div>
<% }) %>



